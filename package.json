{
  "name": "gregives.co.uk",
  "version": "1.2.0",
  "description": "Personal site and portfolio of software engineer Greg Ives.",
  "author": "Greg Ives",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,scss}\" --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:style",
    "forestry": "nuxt -p 8080 -H 0.0.0.0",
    "opengraph": "node -r esm config/opengraph"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{scss,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/sitemap": "^2.4.0",
    "core-js": "^3.11.1",
    "dotenv": "^10.0.0",
    "frontmatter-markdown-loader": "2",
    "highlight.js": "^11.3.1",
    "lazysizes": "^5.3.2",
    "markdown-it": "^12.0.6",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-anchor": "^8.4.1",
    "markdown-it-external-links": "0.0.6",
    "markdown-it-ins": "^3.0.0",
    "markdown-it-mark": "^3.0.0",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-table-of-contents": "^0.6.0",
    "markdown-it-task-lists": "^2.1.1",
    "nuxt": "^2.15.3",
    "postcss": "^8.3.11",
    "sass": "^1.32.12",
    "sass-loader": "^10.2.0",
    "sharp": "^0.29.3",
    "stylelint-config-prettier": "^9.0.3",
    "vue-lazy-hydration": "^2.0.0-beta.4",
    "vue-material-design-icons": "^4.7.1",
    "vue-tabbing": "^0.1.0",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@nuxtjs/eslint-config": "^7.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/google-analytics": "^2.4.0",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/style-resources": "^1.0.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^3.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.1.1",
    "esm": "^3.2.25",
    "husky": "^7.0.4",
    "lint-staged": "^12.0.3",
    "postcss-html": "^1.2.0",
    "prettier": "^2.2.1",
    "stylelint": "^14.1.0",
    "stylelint-config-recommended-scss": "^5.0.1",
    "stylelint-config-recommended-vue": "^1.0.0",
    "typescript": "^4.2.4"
  }
}
