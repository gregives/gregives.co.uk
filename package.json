    "core-js": "^3.11.1",
    "dotenv": "^8.2.0",
    "frontmatter-markdown-loader": "^2.3.0",
    "highlight.js": "^10.7.1",
    "lazysizes": "^5.3.2",
    "markdown-it": "^12.0.6",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-anchor": "^7.1.0",
    "markdown-it-external-links": "0.0.6",
