{
  "name": "gregives.co.uk",
  "version": "1.2.0",
  "description": "Personal site and portfolio of software engineer Greg Ives.",
  "author": "Greg Ives",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
