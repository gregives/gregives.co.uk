{
  "name": "gregives.co.uk",
  "version": "1.0.0",
  "description": "Personal site and portfolio of software developer Greg Ives.",
  "author": "Greg Ives",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "deploy:checks": "git diff-index --quiet HEAD && rimraf pages/resume",
    "deploy:generate": "cross-env NODE_ENV=production nuxt generate",
    "deploy:commit": "git checkout -B deploy && git add -A && git commit --no-verify -m \"Deploy static site\"",
    "deploy:push": "git subtree split --prefix dist deploy && git push -f origin deploy:deploy",
    "deploy:cleanup": "git checkout - && git branch -D deploy",
    "deploy": "npm run deploy:checks && npm run deploy:generate && npm run deploy:commit && npm run deploy:push && npm run deploy:cleanup"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@bazzite/nuxt-optimized-images": "^0.2.1",
    "@nuxtjs/google-analytics": "^2.2.1",
    "@nuxtjs/pwa": "^3.0.0-beta.19",
    "@nuxtjs/sitemap": "^1.3.1",
    "@nuxtjs/style-resources": "^1.0.0",
    "axios": "^0.19.0",
    "frontmatter-markdown-loader": "^2.3.0",
    "highlight.js": "^9.15.10",
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-svgo": "^7.0.0",
    "jimp": "^0.6.8",
    "lazysizes": "^5.1.2",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-anchor": "^5.2.5",
    "markdown-it-ins": "^3.0.0",
    "markdown-it-mark": "^3.0.0",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-task-lists": "^2.1.1",
    "node-sass": "^4.13.0",
    "nuxt": "^2.0.0",
    "responsive-loader": "^1.2.0",
    "sass-loader": "^7.3.1",
    "sqip-loader": "^0.2.0",
    "vue-material-design-icons": "^4.1.0",
    "vue-scrollto": "^2.17.1",
    "vue-tabbing": "^0.1.0",
    "vue-template-compiler": "^2.6.10",
    "vue-template-es2015-compiler": "^1.9.1",
    "webp-loader": "^0.5.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.1.0",
    "babel-eslint": "^10.0.1",
    "cross-env": "^6.0.3",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^2.6.0",
    "lint-staged": "^8.2.1",
    "prettier": "^1.16.4",
    "rimraf": "^3.0.0"
  }
}
